{
  "name": "bookings",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true,
    "strictObjectIDCoercion": true
  },
  "properties": {
    "store_id": {
      "type": "string",
      "required": true
    },
    "slot_id": {
      "type": "string",
      "required": true
    },
    "user_id": {
      "type": "string",
      "required": true
    },
    "status": {
      "type": "string",
      "required": true,
      "default": "CONFIRMED"
    },
    "booking_date": {
      "type": "date",
      "required": true,
      "defaultFn": "now"
    },
    "order_details": {
      "type": "string",
      "required": false
    },
    "order_data": {
      "type": "object",
      "required": false
    }
  },
  "validations": [],
  "relations": {
    "stores": {
      "type": "belongsTo",
      "model": "stores",
      "foreignKey": "store_id",
      "options": {
        "nestRemoting": true
      }
    },
    "users": {
      "type": "belongsTo",
      "model": "users",
      "foreignKey": "user_id",
      "options": {
        "nestRemoting": true
      }
    },
    "stores_slots": {
      "type": "belongsTo",
      "model": "stores_slots",
      "foreignKey": "slot_id",
      "options": {
        "nestRemoting": true
      }
    }
  },
  "acls": [],
  "methods": {}
}