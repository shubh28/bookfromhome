{
  "name": "stores",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true,
    "strictObjectIDCoercion": true
  },
  "properties": {
    "name": {
      "type": "string",
      "required": true
    },
    "email": {
      "type": "string"
    },
    "phone": {
      "type": "string"
    },
    "address": {
      "type": "string",
      "required": true
    },
    "city": {
      "type": "string",
      "required": true
    },
    "locality": {
      "type": "string",
      "required": true
    },
    "isOperating": {
      "type": "boolean",
      "required": true,
      "default": true
    },
    "location": {
      "type": "GeoPoint",
      "required": true
    },
    "store_size": {
      "type": "string"
    },
    "billing_counters": {
      "type": "number",
      "required": true,
      "default": 1
    },
    "gstin": {
      "type": "string"
    },
    "isVerified": {
      "type": "boolean",
      "required": true,
      "default": true
    },
    "store_type": {
      "type": "string",
      "required": true
    },
    "referredBy": {
      "type": "string"
    },
    "updatedBy": {
      "type": "string"
    "created_at": {
      "type": "date",
      "required": true,
      "defaultFn": "now"
    },
    "shop_open_hours": {
      "type": "number",
      "required": true,
      "default": 0
    },
    "shop_open_minutes": {
      "type": "number",
      "required": true,
      "default": 0
    },
    "shop_close_hours": {
      "type": "number",
      "required": true,
      "default": 0
    },
    "shop_close_minutes": {
      "type": "number",
      "required": true,
      "default": 0
    },
    "slot_duration": {
      "type": "number",
      "required": true,
      "default": 15
    }
  },
  "validations": [],
  "relations": {
    "stores_slots": {
      "type": "hasMany",
      "model": "stores_slots",
      "foreignKey": "storeId"
    }
  },
  "acls": [],
  "methods": {}
}
